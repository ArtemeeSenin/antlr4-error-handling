grammar lang;

parse
: expr+ EOF
;

expr
: OPEN_PARENTHESIS literal_value+ CLOSE_PARENTHESIS
;

literal_value
: STRING_LITERAL
;

WHITESPACE : ' ' -> skip ;

STRING_LITERAL
: IDENTIFIER
| SINGLE_QUOTE IDENTIFIER SINGLE_QUOTE
| DOUBLE_QUOTE IDENTIFIER DOUBLE_QUOTE
;

OPEN_PARENTHESIS
: '(' //-> channel(HIDDEN)
;

CLOSE_PARENTHESIS
: ')' //-> channel(HIDDEN)
;

SINGLE_QUOTE
: '\''
;

DOUBLE_QUOTE
: '"'
;

IDENTIFIER
: [a-zA-Z] [a-zA-Z0-9]*
;
